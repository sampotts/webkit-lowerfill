{
    "extends": ["airbnb", "prettier"],
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {},
    "rules": {
        "no-const-assign": 1,
        "no-this-before-super": 1,
        "no-undef": 1,
        "no-unreachable": 1,
        "no-unused-vars": 1,
        "constructor-super": 1,
        "valid-typeof": 1,
        "indent": [2, 4, { "SwitchCase": 1 }],
        //"quotes": [2, "backtick", "avoid-escape"],
        "semi": [2, "always"],
        "eqeqeq": [2, "always"],
        "one-var": [2, "never"],
        "comma-dangle": [2, "always-multiline"],
        "prefer-template": 1
    }
}
